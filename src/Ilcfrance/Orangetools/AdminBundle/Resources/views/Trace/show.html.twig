			{% elseif trace.actionType == constant('AT_DELETE', trace)%}
				{{ include('IlcfranceOrangetoolsAdminBundle:Trace/Sessioninscription:delete.html.twig', {trace: trace}) }}
			{% endif %}
		{% endif %}
	</li>
	{% endfor %}
	<li>
		{{faIco('clock-o bg-gray')}}
	</li>
</ul>