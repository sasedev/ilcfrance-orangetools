{% extends 'SasedevCommonsSharedBundle::tpl.html.twig' %}
{%if menu_active is not defined%}{%set menu_active=''%}{%endif%}
{%if tabActive is not defined%}{%set tabActive=1%}{%endif%}
{%block page_title_content%}{%if browser_page_title is defined%}{{browser_page_title|raw}} - {%endif%}{{sitename}}{%endblock page_title_content%}
{%block body_attributes%} class="hold-transition sidebar-mini fixed skin-orange"{%endblock body_attributes%}
{%block body_content_main%}
			<div class="wrapper admin">
				<header class="main-header">
					{%block mainheader%}
					<nav class="navbar navbar-static-top" role="navigation">
						{%block topmenu%}
						<a href="{{ url('ilcfrance_orangetools_security_homepage') }}" class="logo">
							<span class="logo-mini"><img src="{{absolute_url(asset('/images/logo_small.png'))}}" class="img-responsive" /></span>
							<span class="logo-lg"><img src="{{absolute_url(asset('/images/logo.png'))}}" class="img-responsive" /></span>
						</a>
						<a href="#" class="sidebar-toggle" data-toggle="offcanvas" role="button">
							<span class="sr-only">Toggle navigation</span>
							<span class="icon-bar"></span>
							<span class="icon-bar"></span>
							<span class="icon-bar"></span>
						</a>
						<div class="navbar-collapse collapse navbar-custom-menu-left">
							{{ include('IlcfranceOrangetoolsResBundle:Menu:top.default.html.twig', {menu_active: menu_active}) }}
						</div>
						<div class="navbar-collapse collapse navbar-custom-menu">
							{{ include('IlcfranceOrangetoolsResBundle:Menu:top.right.html.twig', {menu_active: menu_active}) }}
						</div>
						{%endblock topmenu%}
					</nav>
					{%endblock mainheader%}
				</header>
				<aside class="main-sidebar">
					<section class="sidebar">
						{%block leftside%}
						{{ include('IlcfranceOrangetoolsResBundle:Menu:left.html.twig', {menu_active: menu_active}) }}
						{%endblock leftside%}
					</section>
				</aside>
				<div class="content-wrapper">
					{%block contentheader%}
					<section class="content-header">
						<h1>{%if page_title is defined%}{{page_title|raw}}{%else%}{{sitename}}{%endif%}</h1>
						{%block breadcrumb%}
						<ol class="breadcrumb">
							<li class="active"><a href="{{ path('ilcfrance_orangetools_security_homepage')}}"> {{ faIco('home fa-fw')}} {{ 'ilcfrance.orangetools.default.homepage.breadcrumb'|trans }}</a></li>
						</ol>
						{%endblock breadcrumb%}
					</section>
					{%endblock contentheader%}
					<section class="content" id="content">
						{%include 'SasedevCommonsBootstrapBundle::flash.html.twig' with { 'close': true }%}
						{%block maincontent%}
						<div class="box">
							<div class="box-header with-border">
								<h3 class="box-title">Title</h3>
								<div class="box-tools pull-right">
									<button type="button" class="btn btn-box-tool" data-widget="collapse" data-toggle="tooltip" title="Collapse">
										<i class="fa fa-minus fa-fw"></i>
									</button>
									<button type="button" class="btn btn-box-tool" data-widget="remove" data-toggle="tooltip" title="Remove">
										<i class="fa fa-times fa-fw"></i>
									</button>
								</div>
							</div>
							<div class="box-body">
								Start creating your amazing application!
							</div>
							<div class="box-footer">
								Footer
							</div>
						</div>
						{%endblock maincontent%}
					</section>
				</div>

				<footer class="main-footer">
					{%block footercontent%}
					<div class="pull-right hidden-xs">
						<b>Version</b> {{ siteversion }}
					</div>
					<strong>Copyright &copy; {{ 'now'|date('Y')}} <a href="{{ path('ilcfrance_orangetools_security_homepage') }}">{{ sitename }}</a>.</strong> All rights reserved.
					{%endblock footercontent%}
				</footer>
			</div>
{%endblock body_content_main%}
{%block body_scripts%}
{{ parent()}}
<script type="text/javascript">
		<!--
		window.setTimeout(function() { $(".alert").alert('close'); }, 10000);
		{%block begin2_dynamic_jstxt%}
		{%endblock begin2_dynamic_jstxt%}
		-->
		</script>
{%endblock body_scripts%}